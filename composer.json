{
    "name": "jonasschen/laravel-easy-audits",
    "description": "A quick and easy way to audit changes to your Laravel project's database",
    "keywords": [
        "laravel-easy-audits",
        "laravel-audits",
        "laravel",
        "audits",
        "audit",
        "auditing",
        "jonasschen",
        "jonas schen",
        "schen",
        "logs",
        "log"
    ],
    "type": "library",
    "homepage": "https://github.com/jonasschen/laravel-easy-audits",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jonasschen\\LaravelEasyAudits\\": "src"
        }
    },
    "authors": [
        {
            "name": "Jonas Schen",
            "email": "jonasschen@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "laravel/framework": "^11.44",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEasyAuditsServiceProvider"
            ],
            "aliases": {
                "LaravelEasyAudits": "LaravelEasyAuditsFacade"
            }
        }
    }
}
